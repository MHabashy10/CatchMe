{
    "name": "CatchMe",
    "version": "0.0.1",
    "author": "Mohamed Habashy (mohamed.habshey10@gmail.com)",
    "homepage": "http://ionicframework.com/",
    "private": true,
    "main": "./electron/main.js",
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "lint": "ionic-app-scripts lint",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "com.habashy.catchme",
        "asar": true,
        "files": [
            "www/**/*",
            "electron/*",
            "!node_modules/**/*"
        ],
        "win": {
            "target": "nsis",
            "icon": "resources/icon.ico",
            "publisherName": "Habashy Labs, Inc."
        }
    },
    "dependencies": {
        "@angular/common": "^4.4.6",
        "@angular/compiler": "^4.4.6",
        "@angular/compiler-cli": "^4.4.6",
        "@angular/core": "^4.4.6",
        "@angular/forms": "^4.4.6",
        "@angular/http": "^4.4.6",
        "@angular/platform-browser": "^4.4.6",
        "@angular/platform-browser-dynamic": "^4.4.6",
        "@ionic-native/core": "^3.14.0",
        "@ionic-native/splash-screen": "^3.14.0",
        "@ionic-native/status-bar": "^3.14.0",
        "@ionic/storage": "^2.1.1",
        "angularfire2": "^5.0.0-rc.3",
        "cordova-android": "^6.3.0",
        "cordova-plugin-console": "^1.1.0",
        "cordova-plugin-device": "^1.1.6",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2",
        "firebase": "^4.6.1",
        "ionic-angular": "^3.8.0",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionicons": "^3.0.0",
        "promise-polyfill": "6.0.2",
        "rxjs": "^5.5.2",
        "sw-toolbox": "^3.6.0",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@ionic/app-scripts": "^2.1.4",
        "electron": "^1.8.1",
        "electron-builder": "^19.43.4",
        "ionic": "3.18.0",
        "typescript": "^2.6.1"
    },
    "description": "An Ionic project",
    "cordova": {
        "plugins": {
            "cordova-plugin-console": {},
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-whitelist": {},
            "ionic-plugin-keyboard": {}
        },
        "platforms": [
            "android"
        ]
    }
}
